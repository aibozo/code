{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "HarnessResultV1",
  "type": "object",
  "properties": {
    "version": { "type": "string", "const": "v1" },
    "timestamp": { "type": "string" },
    "seed": { "type": "integer" },
    "stages": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "status": { "type": "string", "enum": ["ok", "error"] },
          "metrics": { "type": "object" }
        },
        "required": ["name", "status", "metrics"]
      }
    }
  },
  "required": ["version", "timestamp", "stages"]
}

